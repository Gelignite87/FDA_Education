{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "plugins": ["prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.sass)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
